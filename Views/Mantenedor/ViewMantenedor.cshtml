@{
    ViewBag.Title = "Mantenedor";
}
<h2>Administración</h2>

<div class="tab-content">
    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item active" id="tabUsuarios">
            <a href="#usuarios" role="tab" data-toggle="tab">
                Mantenedor de Usuarios
            </a>
        </li>
        <li class="nav-item">
            <a href="#productos" role="tab" data-toggle="tab" id="tabProductos">
                Mantenedor de Productos
            </a>
        </li>
    </ul>

    <div class="tab-pane fade active in" id="usuarios">

        @Html.Partial("ViewPartialUsuarios")
    </div>
    <div class="tab-pane fade" id="productos">
        @Html.Partial("ViewPartialProductos")
    </div>
</div>



